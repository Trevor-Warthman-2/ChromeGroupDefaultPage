{
  "name": "Tab Group Default Page",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs", "tabGroups", "webNavigation"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/groupDefaultLogoV1-16.png",
      "32": "/images/groupDefaultLogoV1-32.png",
      "48": "/images/groupDefaultLogoV1-48.png",
      "128": "/images/groupDefaultLogoV1-128.png"
    }
  },
  "icons": {
    "16": "/images/groupDefaultLogoV1-16.png",
    "32": "/images/groupDefaultLogoV1-32.png",
    "48": "/images/groupDefaultLogoV1-48.png",
    "128": "/images/groupDefaultLogoV1-128.png"
  },
  "options_page": "options.html"
}
